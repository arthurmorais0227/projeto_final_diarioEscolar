generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Postagem {
  id        Int      @id @default(autoincrement())
  autor     String?
  descricao String?
  data      DateTime @default(now())
  imagem    String?
}


model Alunos {
  id        Int      @id @default(autoincrement())
  nome     String?
  numeroAluno     Int?
  email String?
  senha    String?
}
